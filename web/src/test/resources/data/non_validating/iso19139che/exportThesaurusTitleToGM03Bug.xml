          <gmd:title xsi:type="gmd:PT_FreeText_PropertyType">
            <gco:CharacterString>Skitourenkarte Serie «S» 1:50'000</gco:CharacterString>
            <gmd:PT_FreeText>
              <gmd:textGroup>
                <gmd:LocalisedCharacterString locale="#DE">Skitourenkarte Serie «S» 1:50'000</gmd:LocalisedCharacterString>
              </gmd:textGroup>
              <gmd:textGroup>
                <gmd:LocalisedCharacterString locale="#FR">Carte de randonnées à ski «S» 1:50'000</gmd:LocalisedCharacterString>
              </gmd:textGroup>
              <gmd:textGroup>
                <gmd:LocalisedCharacterString locale="#EN">Ski Tour Map Serie «S» 1:50'000</gmd:LocalisedCharacterString>
              </gmd:textGroup>
              <gmd:textGroup>
