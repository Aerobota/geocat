<beans default-lazy-init="true" xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context" xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.1.xsd">
    <context:annotation-config></context:annotation-config>
    <bean id="main" class="org.mapfish.print.cli.Main"></bean>

    <bean id="pdf" class="org.mapfish.print.output.JasperReportOutputFormat"></bean>
    <bean id="mapPrinter" class="org.mapfish.print.MapPrinter" scope="prototype"></bean>

    <bean id="!localMatch" class="org.mapfish.print.config.AcceptAllMatcher" />
    <bean id="!ipMatch" class="org.mapfish.print.config.AddressHostMatcher" />
    <bean id="!dnsMatch" class="org.mapfish.print.config.DnsHostMatcher" />
    <bean id="!acceptAll" class="org.mapfish.print.config.AcceptAllMatcher" />
    <bean id="!basicAuth" class="org.mapfish.print.config.BasicAuthSecurity" />

    <bean id="!template" class="org.mapfish.print.config.Template" />

    <bean id="!integer" class="org.mapfish.print.attribute.IntegerAttribute" />
    <bean id="!float" class="org.mapfish.print.attribute.FloatAttribute" />
    <bean id="!string" class="org.mapfish.print.attribute.StringAttribute" />
    <bean id="!map" class="org.mapfish.print.attribute.MapAttribute" />
    <bean id="!legend" class="org.mapfish.print.attribute.LegendAttribute" />
    <bean id="!table" class="org.mapfish.print.attribute.TableAttribute" />
    <bean id="!tableList" class="org.mapfish.print.attribute.TableListAttribute" />

    <bean id="!reportBuilder" class="org.mapfish.print.processor.jasper.JasperReportBuilder" />
    <bean id="!prepareLegend" class="org.mapfish.print.processor.jasper.LegendProcessor" />
    <bean id="!prepareTable" class="org.mapfish.print.processor.TableProcessor" />
    <bean id="!prepareTableList" class="org.mapfish.print.processor.jasper.TableListProcessor" />

    <bean id="configurationFactory" class="org.mapfish.print.config.ConfigurationFactory"></bean>

    <bean id="registry" class="org.mapfish.print.servlet.registry.BasicRegistry"></bean>
    <bean id="mapPrinterFactory" class="org.mapfish.print.servlet.ServletMapPrinterFactory"></bean>
    <bean id="jobManager" class="org.mapfish.print.servlet.job.ThreadPoolJobManager"></bean>
    <bean id="printJobFactory" class="org.mapfish.print.servlet.job.FilePrintJob"></bean>
    <bean id="fileReportLoader" class="org.mapfish.print.servlet.job.loader.FileReportLoader"></bean>

</beans>
